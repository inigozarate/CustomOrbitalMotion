FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

   

   comp_domain_atmosphere
    {
        type            totalPressure;
        p0              $internalField;
    }
 

  movingWalls
    {
        type            fixedFluxPressure;
        value           $internalField;
    }


floatingObject
    {
        type            fixedFluxPressure;
        value           $internalField;
    }



    
 comp_domain
    {
        type            fixedFluxPressure;
        value           $internalField;
    } 
 Absorption_zone
    {
  type            fixedFluxPressure;
        value           $internalField;
    }

}



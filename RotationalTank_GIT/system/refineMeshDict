FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      topoSetDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

set waterline;

coordinateSystem global;

globalCoeffs
{
    tan1 (1 0 0);
    tan2 (0 1 0);
    normal (0 0 1);
}

directions
(
normal
    //e3        //
);

useHexTopology  false;

geometricCut    true;

writeMesh       false;



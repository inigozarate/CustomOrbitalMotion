FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
    cylinderToCell
    {
        p1          (0 0 -0.355);
    	p2          (0 0 0.01);
    	radius      0.215;
        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }
);



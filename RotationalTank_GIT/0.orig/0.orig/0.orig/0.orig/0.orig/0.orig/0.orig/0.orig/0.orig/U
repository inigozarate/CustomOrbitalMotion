FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"


    movingWalls
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
 comp_domain
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    comp_domain_atmosphere
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
    }
   Absorption_zone
    {
        type            waveAbsorption3DVelocity;
        nPaddles        25;
        nEdgeMin        0;
        nEdgeMax        1;
        value           uniform (0 0 0);
    }
}


